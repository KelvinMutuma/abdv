<h1>Listing stock_quotes</h1>

<table>
  <thead>
    <tr>
      <th>Security</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stock_quotes.each do |stock_quote| %>
      <tr>
        <td><%= stock_quote.security.security %></td>
        <td><%= link_to 'Show',[@security, stock_quote] %></td>
        <td><%= link_to 'Edit', edit_security_stock_quote_path(@security, stock_quote) %></td>
        <td><%= link_to 'Destroy', [@security, stock_quote], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock quote', new_security_stock_quote_path(@security) %>


